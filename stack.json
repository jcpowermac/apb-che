{
  "id": "apb",
  "name": "Ansible Playbook Bundle",
  "description": "Ansible Playbook Bundle",
  "creator": "che",
  "scope": "general",
  "components": [
    {
      "version": "9.0.1",
      "name": "pip"
    },
    {
      "version": "2.3.1.0",
      "name": "ansible"
    },
    {
      "version": "2.7.13",
      "name": "python"
    },
    {
      "version": "0.1.2",
      "name": "apb"
    }
  ],
  "tags": [
    "ansible",
    "python",
    "docker",
    "openshift",
    "apb",
    "ansible playbook bundle"
  ],
  "workspaceConfig": {
    "environments": {
      "default": {
        "machines": {
          "dev-machine": {
            "attributes": {
              "memoryLimitBytes": "2147483648"
            },
            "servers": {},
            "agents": [
              "org.eclipse.che.exec",
              "org.eclipse.che.terminal",
              "org.eclipse.che.ssh",
              "org.eclipse.che.ws-agent",
              "org.eclipse.che.ls.python",
              "org.eclipse.che.git-credentials"
            ]
          }
        },
        "recipe": {
          "type": "compose",
          "content": "services:\n  dev-machine:\n    image: jcpowermac/apb-che\n    mem_limit: 2147483648\n",
          "contentType": "application/x-yaml"
        }
      }
    },
    "defaultEnv": "default",
    "projects": [],
    "name": "default",
    "commands": [],
    "links": []
  }
}
